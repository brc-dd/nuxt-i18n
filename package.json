{
  "name": "@globalbrain/nuxt-i18n",
  "version": "1.1.0",
  "packageManager": "pnpm@8.15.4",
  "description": "Minimal module for i18n with locale auto-imports & localized routing",
  "author": "Divyansh Singh <divyansh@globalbrains.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/globalbrain/nuxt-i18n.git"
  },
  "bugs": "https://github.com/globalbrain/nuxt-i18n/issues",
  "keywords": [
    "nuxt",
    "nuxt3",
    "i18n"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "default": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "type": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:fail": "eslint .",
    "vitest": "vitest",
    "vitest:run": "vitest run",
    "test": "pnpm run type && pnpm run lint && pnpm run vitest:run",
    "test:fail": "pnpm run type && pnpm run lint:fail && pnpm run vitest:run",
    "release": "release-it",
    "postinstall": "nuxt-module-build prepare",
    "prepack": "nuxt-module-build build"
  },
  "dependencies": {
    "@nuxt/kit": "^3.10.3",
    "knitwork": "^1.0.0"
  },
  "devDependencies": {
    "@globalbrain/eslint-config": "^1.5.2",
    "@nuxt/module-builder": "^0.5.5",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/node": "^20.11.21",
    "eslint": "^8.57.0",
    "nuxt": "^3.10.3",
    "release-it": "^17.1.1",
    "typescript": "^5.3.3",
    "vitest": "1.0.0-beta.2",
    "vue-tsc": "^1.8.27"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
