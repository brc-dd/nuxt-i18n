{
  "name": "@leanera/nuxt-i18n",
  "type": "module",
  "version": "0.4.2",
  "packageManager": "pnpm@8.3.1",
  "description": "Nuxt 3 module for internationalization w/ locale auto-imports & localized routing",
  "author": "LeanERA GmbH <https://github.com/leanera>",
  "license": "MIT",
  "homepage": "https://github.com/leanera/nuxt-i18n#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leanera/nuxt-i18n.git"
  },
  "bugs": "https://github.com/leanera/nuxt-i18n/issues",
  "keywords": [
    "nuxt",
    "nuxt3",
    "i18n"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:types": "tsc --noEmit",
    "release": "bumpp --commit --push --tag",
    "prepare": "nuxi prepare playground",
    "prepack": "nuxt-module-build"
  },
  "dependencies": {
    "@leanera/vue-i18n": "^0.4.2",
    "@nuxt/kit": "^3.4.3",
    "knitwork": "^1.0.0",
    "pathe": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@leanera/nuxt-i18n": "workspace:*",
    "@nuxt/module-builder": "^0.3.1",
    "@nuxt/test-utils": "^3.4.3",
    "@types/node": "^18.16.3",
    "bumpp": "^9.1.0",
    "eslint": "^8.39.0",
    "nuxt": "^3.4.3",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.6.3"
  }
}
