{
  "name": "@byjohann/nuxt-i18n",
  "type": "module",
  "version": "1.0.2",
  "packageManager": "pnpm@8.7.4",
  "description": "Minimal module for i18n with locale auto-imports & localized routing",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/nuxt-i18n#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nuxt-i18n.git"
  },
  "bugs": "https://github.com/johannschopplich/nuxt-i18n/issues",
  "keywords": [
    "nuxt",
    "nuxt3",
    "i18n"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "release": "bumpp --commit --push --tag",
    "prepare": "nuxi prepare playground",
    "prepack": "nuxt-module-build"
  },
  "dependencies": {
    "@byjohann/vue-i18n": "^1.0.2",
    "@nuxt/kit": "^3.7.1",
    "knitwork": "^1.0.0",
    "pathe": "^1.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@byjohann/nuxt-i18n": "workspace:*",
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/test-utils": "^3.7.1",
    "@types/node": "^20.5.9",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "nuxt": "^3.7.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3",
    "vue-tsc": "^1.8.10"
  }
}
