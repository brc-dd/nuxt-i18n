{
  "name": "@leanera/nuxt-i18n",
  "type": "module",
  "version": "0.2.4",
  "packageManager": "pnpm@7.13.3",
  "description": "Lightweight internationalization module for Nuxt 3",
  "author": "LeanERA GmbH <https://github.com/leanera>",
  "license": "MIT",
  "homepage": "https://github.com/leanera/nuxt-i18n#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leanera/nuxt-i18n.git"
  },
  "bugs": "https://github.com/leanera/nuxt-i18n/issues",
  "keywords": [
    "nuxt",
    "nuxt3",
    "i18n"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag",
    "prepare": "nuxi prepare playground",
    "prepack": "nuxt-module-build"
  },
  "dependencies": {
    "@leanera/vue-i18n": "^0.2.1",
    "@nuxt/kit": "^3.0.0-rc.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@leanera/nuxt-i18n": "workspace:*",
    "@nuxt/module-builder": "^0.1.7",
    "bumpp": "^8.2.1",
    "eslint": "^8.25.0",
    "nuxt": "^3.0.0-rc.11",
    "typescript": "^4.8.4",
    "vue-tsc": "^1.0.2"
  }
}
